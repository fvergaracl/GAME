{
  "realm": "${KEYCLOAK_REALM}",
  "enabled": true,
  "clients": [
    {
      "clientId": "${KEYCLOAK_CLIENT_ID}",
      "name": "GAME API",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "${KEYCLOAK_CLIENT_SECRET}",
      "protocol": "openid-connect",
      "publicClient": false,
      "bearerOnly": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true,
      "redirectUris": [
        "http://localhost:8000/*",
        "http://localhost/*",
        "https://localhost/*",
        "https://localhost:8000/*"
      ],
      "webOrigins": [
        "http://localhost:8000",
        "http://localhost",
        "https://localhost:8000",
        "https://localhost"
      ],
      "defaultClientScopes": [
        "profile",
        "email",
        "roles"
      ],
      "optionalClientScopes": [
        "offline_access",
        "address",
        "phone",
        "microprofile-jwt"
      ]
    }
  ],
  "roles": {
    "client": {
      "${KEYCLOAK_CLIENT_ID}": [
        {
          "name": "AdministratorGAME",
          "description": "Can create resources in GAME API"
        }
      ]
    }
  },
  "users": [
    {
      "username": "game_admin",
      "email": "game_admin@example.com",
      "enabled": true,
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "${KEYCLOAK_USER_WITH_ROLE_PASSWORD}",
          "temporary": false
        }
      ],
      "clientRoles": {
        "${KEYCLOAK_CLIENT_ID}": [
          "AdministratorGAME"
        ]
      }
    },
    {
      "username": "game_user",
      "email": "game_user@example.com",
      "enabled": true,
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "${KEYCLOAK_USER_NO_ROLE_PASSWORD}",
          "temporary": false
        }
      ]
    }
  ]
}
